{
  "name": "movio-react-components",
  "version": "0.1.0",
  "description": "Movio's React Component Lib",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress --colors --host 0.0.0.0",
    "build": "webpack --progress --colors",
    "test": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "lint": "eslint src test",
    "format": "eslint src test --fix; exit 0",
    "check": "npm run lint && npm run test",
    "ui:start": "node ./node_modules/ui-harness/start",
    "ui:build": "node ./node_modules/ui-harness/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/movio/movio-react-components"
  },
  "keywords": [
    "movio",
    "react",
    "component"
  ],
  "contributors": [
    {
      "name": "Kalman Bekesi",
      "url": "https://github.com/kalmanb"
    },
    {
      "name": "Raghuvir Kasturi",
      "url": "https://github.com/d4ncer"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-movio": "^1.0.2",
    "babel-runtime": "^6.9.2",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.2.5",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "5.1.1",
    "file-loader": "^0.8.5",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.7",
    "ui-harness": "^3.9.4",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "react": "^15.1.0",
    "react-hot-loader": "^1.3.0"
  }
}

{
  "name": "movio-react-components",
  "version": "0.1.3",
  "description": "Movio's React Component Lib",
  "scripts": {
    "dev": "BABEL_ENV=dev webpack-dev-server --progress --colors",
    "test": "BABEL_ENV=test jest --config=jest.config.json",
    "test:watch": "BABEL_ENV=test jest --config=jest.config.json --watchAll",
    "cov": "flow-coverage-report -i 'src/**/*.js' -t html && yarn test",
    "flow:cov": "flow-coverage-report -i 'src/**/*.js'",
    "lint": "eslint src",
    "lint:style": "stylelint 'src/**/*.css'",
    "vet": "yarn lint && yarn lint:style && yarn test && yarn flow",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/movio/movio-react-components"
  },
  "keywords": [
    "movio",
    "react",
    "components",
    "library"
  ],
  "contributors": [
    {
      "name": "Kalman Bekesi",
      "url": "https://github.com/kalmanb"
    },
    {
      "name": "Raghuvir Kasturi",
      "url": "https://github.com/d4ncer"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.1.1",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.47.0",
    "flow-coverage-report": "^0.3.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.5",
    "postcss-reporter": "^3.0.0",
    "prettier": "^1.3.1",
    "react-addons-test-utils": "^15.5.1",
    "react-hot-loader": "^1.3.1",
    "react-test-renderer": "^15.5.4",
    "rucksack-css": "^0.9.1",
    "style-loader": "^0.18.1",
    "stylelint": "^7.10.1",
    "svg-url-loader": "^2.0.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "yargs": "^8.0.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "css-module-flow": "^1.0.0",
    "lodash": "^4.15.0",
    "normalize.css": "^7.0.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-tether": "^0.5.7"
  }
}

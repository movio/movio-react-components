{
  "name": "movio-react-components",
  "version": "0.1.2",
  "description": "Movio's React Component Lib",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --progress --colors --host 0.0.0.0",
    "test": "karma start --single-run; exit 0",
    "test:ci": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "lint": "eslint src; exit 0",
    "lint:ci": "eslint src",
    "format": "eslint src --fix; exit 0",
    "lint:style": "stylelint 'src/**/*.css'; exit 0",
    "lint:style:ci": "stylelint 'src/**/*.css'",
    "check": "npm run lint:ci && npm run lint:style:ci && npm run test:ci",
    "docs": "jsdoc -c jsdoc.conf.json ./src/components",
    "serve:docs": "http-server ./docs -o -p 8081",
    "serve:coverage": "http-server ./coverage/report -o -p 8082"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/movio/movio-react-components"
  },
  "keywords": [
    "movio",
    "react",
    "component"
  ],
  "contributors": [
    {
      "name": "Kalman Bekesi",
      "url": "https://github.com/kalmanb"
    },
    {
      "name": "Raghuvir Kasturi",
      "url": "https://github.com/d4ncer"
    },
    {
      "name": "Wasiq Kashkari",
      "url": "https://github.com/qisaw"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-istanbul-loader": "^0.1.0",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-movio": "^1.2.1",
    "babel-runtime": "^6.11.6",
    "carte-blanche": "^0.3.1",
    "carte-blanche-react-plugin": "^0.3.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.23.1",
    "cssnano": "^3.7.3",
    "cssrecipes-defaults": "^0.5.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.4.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "html-webpack-plugin": "^2.22.0",
    "http-server": "^0.9.0",
    "ink-docstrap": "^1.2.1",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "karma": "^1.1.2",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.0.5",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.5.3",
    "phantomjs-prebuilt": "^2.1.8",
    "postcss-cssnext": "^2.7.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.9.1",
    "postcss-reporter": "^1.4.1",
    "react-addons-test-utils": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "rucksack-css": "^0.8.6",
    "sinon": "^2.0.0-pre",
    "style-loader": "^0.13.1",
    "stylelint": "^7.0.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "yargs": "^4.8.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.14.0",
    "normalize.css": "^4.2.0",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  }
}

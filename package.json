{
  "name": "movio-react-components",
  "version": "0.1.3",
  "description": "Movio's React Component Lib",
  "scripts": {
    "start": "webpack-dev-server --progress --colors",
    "test": "karma start --single-run; exit 0",
    "test:ci": "karma start --single-run",
    "test:watch": "karma start --auto-watch",
    "lint": "eslint src; exit 0",
    "lint:ci": "eslint src",
    "format": "eslint src --fix; exit 0",
    "lint:style": "stylelint 'src/**/*.css'; exit 0",
    "lint:style:ci": "stylelint 'src/**/*.css'",
    "check": "npm run lint:ci && npm run lint:style:ci && npm run test:ci",
    "serve:coverage": "http-server ./coverage/report -o -p 8082"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/movio/movio-react-components"
  },
  "keywords": [
    "movio",
    "react",
    "component"
  ],
  "contributors": [
    {
      "name": "Kalman Bekesi",
      "url": "https://github.com/kalmanb"
    },
    {
      "name": "Raghuvir Kasturi",
      "url": "https://github.com/d4ncer"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-movio": "^1.2.1",
    "babel-runtime": "^6.23.0",
    "chalk": "^1.1.3",
    "css-loader": "^0.26.2",
    "cssnano": "^3.10.0",
    "eslint": "^3.17.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "flow-bin": "^0.40.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.2",
    "postcss-reporter": "^3.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rucksack-css": "^0.9.1",
    "style-loader": "^0.13.2",
    "stylelint": "^7.9.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1",
    "yargs": "^6.6.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "lodash": "^4.15.0",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-tether": "^0.5.2"
  }
}
